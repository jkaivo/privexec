CC=c99
LD=$(CC)
CFLAGS=-Wall -Wextra -Wpedantic -Werror -g
LDFLAGS=
LDLIBS=
SRCDIR=.
OBJDIR=.
BINDIR=$(OBJDIR)

all: $(BINDIR)/privexec

clean:
	rm -f $(BINDIR)/privexec $(OBJDIR)/*.o

$(BINDIR)/privexec: $(OBJDIR)/privexec.o
$(OBJDIR)/privexec.o: $(SRCDIR)/privexec.c
	$(CC) $(CFLAGS) -o $@ -c $(SRCDIR)/privexec.c

$(BINDIR)/privexec:
	$(LD) $(LDFLAGS) -o $@ $(OBJDIR)/*.o $(LDLIBS)
